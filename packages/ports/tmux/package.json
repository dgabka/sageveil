{
  "name": "@sageveil/tmux",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsx {projectRoot}/src/index.ts",
          "env": {
            "OUTPUT_DIR": "{workspaceRoot}/dist/{projectRoot}"
          }
        },
        "outputs": [
          "{workspaceRoot}/dist/{projectRoot}"
        ]
      },
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "command": "rm -rf {projectRoot}/dist"
        }
      }
    }
  }
}
